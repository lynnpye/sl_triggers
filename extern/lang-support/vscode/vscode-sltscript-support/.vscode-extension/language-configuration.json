{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string", "comment"]
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "{",
      "close": "}",
      "notIn": ["string", "comment"]
    }
  ],
  "surroundingPairs": [
    ["\"", "\""],
    ["[", "]"],
    ["{", "}"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*beginsub\\b",
      "end": "^\\s*endsub\\b"
    }
  },
  "wordPattern": "\\$?[a-zA-Z_][a-zA-Z0-9_.]*",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*beginsub\\b",
    "decreaseIndentPattern": "^\\s*endsub\\b"
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*beginsub\\b.*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*endsub\\b.*$",
      "action": {
        "indent": "outdent"
      }
    }
  ]
}