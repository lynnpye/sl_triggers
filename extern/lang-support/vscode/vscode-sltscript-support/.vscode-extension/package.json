{
  "name": "sltscript-language",
  "displayName": "SLTScript Language Support",
  "description": "Syntax highlighting for SLTScript files",
  "version": "1.0.0",
  "publisher": "hextun",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Programming Languages"],
  "keywords": ["sltscript", "skyrim", "scripting", "syntax"],
  "contributes": {
    "languages": [{
      "id": "sltscript",
      "aliases": ["SLTScript", "sltscript"],
      "extensions": [".sltscript"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sltscript",
      "scopeName": "source.sltscript",
      "path": "./syntaxes/sltscript.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lynnpye/sl_triggers"
  }
}